{
    "env": {
      "dev": {
        "presets": ["@babel/preset-env", "@babel/preset-react"],
        "plugins": ["react-hot-loader/babel", "@babel/plugin-proposal-class-properties"]
      },
      "production": {
        "presets": [
          ["@babel/preset-react", {
            "modules": "commonjs"
          }]
        ],
        "plugins": ["emotion"]
      }
    }
  }