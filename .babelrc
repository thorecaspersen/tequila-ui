{
    "env": {
      "dev": {
        "presets": ["@babel/preset-env", "@babel/preset-react"],
        "plugins": ["react-hot-loader/babel", "@babel/plugin-proposal-class-properties", ["prismjs", {
          "languages": ["javascript", "css", "markup", "jsx"],
          "plugins": ["line-numbers"],
          "theme": "okaidia",
          "css": true
      }]]
      },
      "production": {
        "presets": [
          ["@babel/preset-react", {
            "modules": "commonjs"
          }]
        ],
        "plugins": [
          ["emotion"]
        ]
      }
    }
  }