{
    "env": {
      "dev": {
        "presets": ["@babel/preset-env", "@babel/preset-react"],
        "plugins": ["react-hot-loader/babel", "@babel/plugin-proposal-class-properties"]
      },
      "production": {
        "presets": [
          ["@emotion/babel-preset-css-prop", {
            "autoLabel": true,
            "labelFormat": "[local]"
          }],
          ["@babel/preset-react", {
            "modules": "commonjs"
          }]
        ],
        "plugins": ["@babel/plugin-transform-react-jsx"]
      }
    }
  }